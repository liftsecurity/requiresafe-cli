{
  "name": "requiresafe",
  "description": "the requiresafe.com command line interface tool",
  "version": "1.5.0",
  "author": "^lift security",
  "bin": {
    "requiresafe": "bin/requiresafe"
  },
  "dependencies": {
    "chalk": "1.1.1",
    "es6-promise": "3.0.2",
    "find-root": "0.1.1",
    "inquirer": "0.10.1",
    "is-email": "0.1.1",
    "jsonwebtoken": "5.0.5",
    "minimist": "1.2.0",
    "rc": "1.1.1",
    "requiresafe-sdk": "3.0.4",
    "semver": "5.0.3"
  },
  "devDependencies": {
    "@liftsecurity/pre-commit": "1.0.1",
    "code": "1.5.0",
    "lab": "5.18.1",
    "shrinkydink": "1.0.0"
  },
  "keywords": [
    "security",
    "requiresafe"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "configure-local": "node -e \"var uc = require('./lib/update-config'); uc('local', true, process.exit)\"",
    "setup-offline": "curl -sS https://api.requiresafe.com/api/v1/advisories -o advisories.json",
    "test": "lab -a code -L",
    "shrinkwrap": "npm shrinkwrap && shrinkydink"
  }
}
