    "subcommand": "^2.0.3",
    "wreck": "^7.0.0"
  },
  "devDependencies": {
    "code": "^2.0.1",
    "eslint": "^1.8.0",
    "eslint-config-nodesecurity": "^1.1.0",
    "eslint-plugin-hapi": "^4.0.0",
